<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="TypeBean"
            type="com.leanper.jetpackdemo.bean.TypeBean" />
    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Arch.databinding.ViewBindingFragment">

        <ImageView
            app:layout_constraintTop_toBottomOf="@id/ln"
            android:layout_margin="@dimen/qb_px_60"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:id="@+id/iv_carbig"
            android:src="@mipmap/carbig"
            android:transitionName="carbig"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></ImageView>
        <!-- TODO: Update blank fragment layout -->
        <TextView

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@{TypeBean.name}" />

    </FrameLayout>
</layout>