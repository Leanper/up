<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leanper.jetpackdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".MyApplication"
        android:theme="@style/Theme.JetpackDemo">
        <activity android:name=".ui.ui.layouts.Layout2Activity"></activity>
        <activity android:name=".Arch.datastore.DataStoreActivity" />
        <activity android:name=".Arch.navigation.NavAnimationActivity" />
        <activity android:name=".ui.ui.layouts.LayoutActivity" />
        <activity android:name=".Arch.navigation.NavigationActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>